package com.vk.model;

public class InspectionQuestion {
    private Integer id;

    private Integer firstQuestionId;

    private Integer twoQuestionId;

    private Integer score;

    private Integer status;

    private String text;

    private Integer recordId;

    public Integer getId() {
        return id;
    }

    public void setId(Integer id) {
        this.id = id;
    }

    public Integer getFirstQuestionId() {
        return firstQuestionId;
    }

    public void setFirstQuestionId(Integer firstQuestionId) {
        this.firstQuestionId = firstQuestionId;
    }

    public Integer getTwoQuestionId() {
        return twoQuestionId;
    }

    public void setTwoQuestionId(Integer twoQuestionId) {
        this.twoQuestionId = twoQuestionId;
    }

    public Integer getScore() {
        return score;
    }

    public void setScore(Integer score) {
        this.score = score;
    }

    public Integer getStatus() {
        return status;
    }

    public void setStatus(Integer status) {
        this.status = status;
    }

    public String getText() {
        return text;
    }

    public void setText(String text) {
        this.text = text == null ? null : text.trim();
    }

    public Integer getRecordId() {
        return recordId;
    }

    public void setRecordId(Integer recordId) {
        this.recordId = recordId;
    }
}