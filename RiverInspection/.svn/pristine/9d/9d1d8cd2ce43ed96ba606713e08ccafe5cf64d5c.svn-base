<!DOCTYPE html><html lang="en" xmlns:th="http://www.thymeleaf.org"><head>    <meta charset="UTF-8" />    <meta name="renderer" content="webkit" />    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0,user-scalable=0,uc-fitscreen=yes" />    <meta name="apple-mobile-web-app-capable" content="yes" />    <meta name="apple-mobile-web-app-status-bar-style" content="black" />    <meta name="format-detection" content="telephone=no" />    <title>设置</title>    <link rel="stylesheet" type="text/css" href="../css/miniMobile.css"/>    <script type="text/javascript" src="../js/zepto.min.js"></script>    <script type="text/javascript" src="../js/miniMobile.js"></script>    <link rel="stylesheet" type="text/css" href="../plugins/mobileSelect/mobileSelect.css">    <script src="../plugins/mobileSelect/mobileSelect.js" type="text/javascript"></script>    <script src="../plugins/icheck/icheck.js" type="text/javascript" charset="utf-8"></script>    <script src="../plugins/noUiSlider.10.0.0/nouislider.js" type="text/javascript" charset="utf-8"></script>    <link rel="stylesheet" type="text/css" href="../plugins/switchery/switchery.css"/>    <script src="../plugins/switchery/switchery.js" type="text/javascript" charset="utf-8"></script>    <link rel="stylesheet" type="text/css" href="../css/style.css">    <script type="text/javascript" src="../plugins/layer_mobile/layer.js"></script></head><body>    <input type="hidden" id="userId" th:value="${userId}">    <div class="pad-top">        <div class="top">            <a onclick="javascript:history.go(-1)"><img src="../img/back.png" alt=""></a>            设置        </div>        <div class="setForm">            <div class="setLabel">                <span>修改密码</span>                <img src="../img/icon-right.png" alt="">            </div>        </div>        <div class="setPsd">            <div class="setLabel">                <span>旧密码</span>                <input id="oldPsd" type="password">            </div>            <div class="setLabel">                <span>新密码</span>                <input id="psd" type="password">                <a onclick="commit()">完成</a>            </div>        </div>        <a class="exit">退出登录</a>    </div></body><script>    $(".setForm").click(function(){        $(this).hide();        $(".exit").hide();        $(".setPsd").show();        $("#oldPsd").focus();    })    function commit(){        layer.open({            content: "您是否修改密码",            btn: ['确定', '取消'],            skin: 'footer',            yes: function() {                var oldPsd = $("#oldPsd").val().trim();                var psd = $("#psd").val().trim();                var userId = $("#userId").val().trim();                if(oldPsd == ''){                    layer.open({                        content: '旧密码不能为空',                        skin: 'footer',                        time:1                    });                    return false;                }else if(psd == ''){                    layer.open({                        content: '新密码不能为空',                        skin: 'footer',                        time:1                    });                    return false;                }                $.post("/user/updatePassword",                    {                        oldPassword:oldPsd,                        newPassword:psd,                        userId:userId                    },                    function(data,status){                        layer.open({                            content: data.msg,                            skin: 'msg',                            time:1                        });                        console.log(data);                        console.log(photoFiles);                        setTimeout("window.location.href='../page/adminIndex'",1000)                    }                );            }        });    }</script></html>