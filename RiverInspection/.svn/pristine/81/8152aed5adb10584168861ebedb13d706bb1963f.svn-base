}
.o-h dt span{
	color: #6BBB88;
    float: left;
    margin-right: 10px;
    font-size: 45px;
    height: 20px;
    line-height: 20px;
    overflow: hidden;
    margin-top: 15px;
}
.o-h dd{
	display: none;
}
.o-h .check-box span{
	margin-right: 0;
}
