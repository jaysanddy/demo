}
.river-qj li{
	border:1px solid #DEDEDE;
	border-radius: 3px;
	padding:0 10px;
	height:40px;
	line-height: 40px;
	margin-right:20px;
	margin-bottom: 10px;
	float:left;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
	max-width:100%;
	position: relative;
}
.river-qj li b{
	font-weight: normal;
}
.river-qj li.active{
	color:#6BBB88;
	border-color: #6BBB88;
	padding-right: 50px;
}
.river-qj li:last-child{
	padding-right: 10px;
}
.river-qj li input{
	width: 25px;
    height: 25px;
    border: 1px solid #FF9493;
    color: #FF9493;
    margin-left: 10px;
    border-radius: 2px;
    text-align: right;
    padding: 0 5px;
    outline: none;
    display: none;
    float: right;
    margin-top: 6px;
    position: absolute;
    top: 0;
    right: 10px;
}
.river-qj li.active input,.river-qj li.active span{
	display: block;
}
.river-qj li span{
	position: absolute;
	right:25px;
	top:-3px;
	z-index: 2;
	color:#FF9493;
	display: none;
}
.river-qj{
	float:left;
	width:100%;
}
.river-qj li a.del{
	float: right;
    width: 20px;
    height: 20px;
    line-height: 15px;
    position: absolute;
    right: 5px;
    top: 10px;
    z-index: 1;
    border-radius: 50%;
    color: #f00;
    background: #f2f2f2;
    text-align: center;
    font-size: 25px;
}
